{
  "listen": {
    "host": "",
    "port": 15000
  },
  "server": {
    "onetimesecret_public_server": "",
    "item_link_server": "",
    "rate_limit_max_requests": 100,
    "http_cache_size_mb": 50
  },
  "passweaverapi_url": "http://localhost:3000/api/v1",
  "company_name": "MyCompany",
  "log": {
    "dir": "./log",
    "rotation": "1d",
    "retention": 14
  },
  "https": {
    "enabled": false,
    "certificate": "/path/to/certificate",
    "private_key": "/path/to/private_key",
    "hsts": false
  },
  "redis": {
    "enabled": true,
    "url": "redis://localhost:6379"
  },
  "onetimetokens": {
    "default_hours": 168,
    "enable_public_creation": true
  },
  "folders": {
    "user_managed": true
  },
  "generate_password_length": 15,
  "auth": {
    "google_oauth2": {
      "enabled": false
    }
  }
}