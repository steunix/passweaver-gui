<sl-dialog id="apikeysecretdialog" label="API Key secret" class="dialog-deny-close">
  <sl-alert open variant="warning">
    This is the new API Key: <strong>copy the secret</strong> before closing this dialog, because you won't be able to see it again
    <sl-icon slot="icon" name="exclamation-octagon"></sl-icon>
  </sl-alert>
  <br/>
  <sl-input id="apikeynewid" disabled label="API key">
    <sl-copy-button slot="suffix" from="apikeynewid.value"></sl-copy-button>
  </sl-input>
  <sl-input id="apikeysecret" disabled label="Secret">
    <sl-copy-button slot="suffix" from="apikeysecret.value"></sl-copy-button>
  </sl-input>
  <sl-button id="apikeysecretclose" slot="footer">Close</sl-button>
</sl-dialog>
