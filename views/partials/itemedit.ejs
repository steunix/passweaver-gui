<sl-dialog id="itemeditdialog" label="Edit item">
  <sl-icon-button slot="header-actions" title="Copy item link" name="link-45deg" id="itemeditcopylink"></sl-icon-button>
  <sl-visually-hidden id="itemeditid"></sl-visually-hidden>
  <sl-input label="Title" id="edittitle" name="edittitle" required="required" autofocus maxlength="200"></sl-input>
  <sl-select label="Type" id="edittype" name="edittype" clearable></sl-select>
  <sl-textarea label="Description" id="editdescription" name="editdescription"></sl-textarea>
  <sl-input label="Email" id="editemail" name="editemail"></sl-input>
  <sl-input label="URL / Host" id="editurl" name="editurl"></sl-input>
  <sl-input label="User" id="edituser" name="edituser"></sl-input>
  <sl-input label="Password" id="editpassword" name="editpassword" type="password" password-toggle></sl-input>
  <sl-button id="itemeditsave" disabled="disabled" slot="footer" variant="primary">Save</sl-button>
  <sl-button id="itemeditcancel" slot="footer">Cancel</sl-button>
</sl-dialog>