<wa-dialog id="apikeydialog" label="API Key" light-dismiss>
  <div class="wa-stack">
    <wa-input id="apikeyid" style="display: none;"></wa-input>
    <wa-input id="apikeydescription" label="Description" required autofocus maxlength="50"></wa-input>
    <wa-select id="apikeyuserid" label="User" required></wa-select>
    <wa-input id="apikeyexpiresat" label="Expire date" required placeholder="YYYY-MM-DD"></wa-input>
    <wa-select id="apikeyactive" label="Status" required>
      <wa-option value="A">Active</wa-option>
      <wa-option value="I">Inactive</wa-option>
    </wa-select>
  </div>
  <wa-button id="apikeysave" slot="footer" variant="brand" disabled="disabled">Save</wa-button>
  <wa-button id="apikeycancel" slot="footer">Cancel</wa-button>
</wa-dialog>
