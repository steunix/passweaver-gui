<div id="userpicker" class="modal" tabindex="-1" data-bs-backdrop="static" data-bs-keyboard="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header v-themebg">
        <h5 class="mb-0">Choose user</h5>
      </div>
      <div class="modal-body">
        <div class="mb-3">
          <div class="input-group">
            <input type="text mb-3" class="form-control" autofocus id="userpickersearch" name="userpickersearch" placeholder="Search user">
            <button class="btn btn-secondary" type="button" id="button-addon1"><span class="fa-solid fa-search"></span></button>
          </div>
          <table class="table mt-3" id="userpickertable">
            <thead>
              <tr>
                <th class="col-1">Choose</th>
                <th class="col-2">Login</th>
                <th>Name</th>
              </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
      </div>
      <div class="modal-footer">
        <button id="cancel" type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
      </div>
    </div>
  </div>
</div>