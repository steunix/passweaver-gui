<wa-dialog id="confirmdialog" with-header with-footer label="Dialog" class="dialog-deny-close">
  <span id="confirmdialogtext"></span>
  <wa-button slot="footer" id="confirmok" variant="brand">OK</wa-button>
  <wa-button slot="footer" data-dialog="close" id="confirmcancel">Cancel</wa-button>
</wa-dialog>