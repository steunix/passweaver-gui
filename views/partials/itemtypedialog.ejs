<sl-dialog id="itemtypedialog" label="Item type" class="dialog-deny-close">
  <sl-input type="hidden" id="itdid" style="display:none;"></sl-input>
  <sl-input id="itddescription" label="Description" required autofocus maxlength="20"></sl-input>
  <sl-input id="itdicon" label="Icon" maxlength="50"></sl-input>
  <sl-button id="itdsave" slot="footer" variant="primary" disabled="disabled">Save</sl-button>
  <sl-button id="itdcancel" slot="footer">Cancel</sl-button>
</sl-dialog>
