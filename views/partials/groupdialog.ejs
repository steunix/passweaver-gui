<sl-dialog id="groupdialog" label="Group" class="dialog-deny-close">
  <sl-input id="groupdialogid" style="display:none;"></sl-input>
  <sl-input id="groupdescription" label="Description" required autofocus maxlength="100"></sl-input>
  <sl-button id="groupsave" slot="footer" variant="primary" disabled="disabled">Save</sl-button>
  <sl-button id="groupcancel" slot="footer">Cancel</sl-button>
</sl-dialog>
