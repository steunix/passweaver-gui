<wa-dialog id="folderdialog"  class="dialog-footer" light-dismiss label="Folder" class="dialog-deny-close">
  <wa-input id="folderdialogid" style="display:none;"></wa-input>
  <wa-input id="folderdescription" label="Description" required autofocus maxlength="100"></wa-input>
  <wa-button id="foldersave" slot="footer" variant="brand" disabled>Save</wa-button>
  <wa-button id="foldercancel" data-dialog="close" slot="footer" appearance="outlined">Cancel</wa-button>
</wa-dialog>
