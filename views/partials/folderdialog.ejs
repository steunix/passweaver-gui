<sl-dialog id="folderdialog" label="Folder" class="dialog-deny-close">
  <sl-input id="folderdialogid" style="display:none;"></sl-input>
  <sl-input id="folderdescription" label="Description" required autofocus maxlength="100"></sl-input>
  <sl-button id="foldersave" slot="footer" variant="primary" disabled="disabled">Save</sl-button>
  <sl-button id="foldercancel" slot="footer">Cancel</sl-button>
</sl-dialog>
