<wa-dialog id="itemtypenewdialog" label="New item type" class="dialog-deny-close">
  <wa-input id="itemtypenewdescription" label="Description" required autofocus maxlength="20"></wa-input>
  <wa-input id="itemtypenewicon" label="Icon" maxlength="50"></wa-input>
  <wa-button id="itemtypenewsave" slot="footer" variant="primary" disabled="disabled">Save</wa-button>
  <wa-button id="itemtypenewcancel" slot="footer">Cancel</wa-button>
</wa-dialog>
