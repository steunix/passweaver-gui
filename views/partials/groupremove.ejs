<div id="removegroupdialog" class="modal" tabindex="-1" data-bs-backdrop="static" data-bs-keyboard="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header bg-dark">
        <h5 class="text-light mb-0">Delete group</h5>
      </div>
      <div class="modal-body">
      <strong><span class="text-danger">Are you sure you want to delete this group?</span></strong>
      <input type="hidden" id="groupremoveid">
      </div>
      <div class="modal-footer">
        <button id="groupremove" type="button" class="btn btn-danger" onclick="javascript:groupRemove()">Delete</button>
        <button id="cancel" type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
      </div>
    </div>
  </div>
</div>